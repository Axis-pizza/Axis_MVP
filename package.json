{
  "name": "axis-mvp-monorepo",
  "private": true,
  "scripts": {
    "dev:api": "pnpm --filter axis-api dev",
    "deploy:api": "pnpm --filter axis-api deploy",
    "typegen:api": "pnpm --filter axis-api cf-typegen",
    "dev:mock": "pnpm --filter axis-mock dev",
    "build:mock": "pnpm --filter axis-mock build",
    "start:mock": "pnpm --filter axis-mock start",
    "lint:mock": "pnpm --filter axis-mock lint",
    "dev": "pnpm --recursive --parallel dev"
  },
  "pnpm": {
    "overrides": {
      "@solana/web3.js": "1.98.4"
    }
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/node": "^25.0.10",
    "pnpm": "^10.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@meteora-ag/dlmm": "^1.9.3",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bn.js": "^5.2.2"
  }
  
}
